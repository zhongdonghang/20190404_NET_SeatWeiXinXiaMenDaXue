@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>剩余座位</title>
    <link rel="stylesheet" href="//g.alicdn.com/msui/sm/0.6.2/css/sm.min.css">
    <link rel="stylesheet" href="//g.alicdn.com/msui/sm/0.6.2/css/sm-extend.min.css">
</head>
<body>
    <div class="page">
        <header class="bar bar-nav">
            @*<a class="button button-link button-nav pull-left" href="/User/Index" data-transition='slide-out'>
                <span class="icon icon-left"></span>
                返回
            </a>*@
            <h1 class="title">剩余座位</h1>
            <a class="icon icon-home pull-right open-panel" href="/User/Index"></a>
        </header>
        <div class="content native-scroll">
            
            <div class="list-block" style="margin-top: 0px;">
                <ul>
                    @foreach (var item in ViewBag.List)
                    {
                        <li class="item-content">
                            <div class="item-media"><i class="icon icon-f7"></i></div>
                            <div class="item-inner">
                                <div class="item-title">@item.RoomName</div>
                                <div class="item-after">剩余座位: @item.ResidueSeat 个</div>
                            </div>
                        </li>
                    }

                </ul>
            </div>

        </div>
    </div>
    <script type='text/javascript' src='//g.alicdn.com/sj/lib/zepto/zepto.min.js' charset='utf-8'></script>
    <script type='text/javascript' src='//g.alicdn.com/msui/sm/0.6.2/js/sm.min.js' charset='utf-8'></script>
    <script type='text/javascript' src='//g.alicdn.com/msui/sm/0.6.2/js/sm-extend.min.js' charset='utf-8'></script>
</body>
</html>
